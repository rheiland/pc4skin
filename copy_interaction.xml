<PhysiCell_settings version="devel-version">
	<domain>
		<x_min>-400</x_min>
		<x_max>400</x_max>
		<y_min>-400</y_min>
		<y_max>400</y_max>
		<z_min>-10</z_min>
		<z_max>10</z_max>
		<dx>20</dx>
		<dy>20</dy>
		<dz>20</dz>
		<use_2D>true</use_2D>
	</domain>
	
	<overall>
		<max_time units="min">7200</max_time> 
		<time_units>min</time_units>
		<space_units>micron</space_units>
	
		<dt_diffusion units="min">0.01</dt_diffusion>
		<dt_mechanics units="min">0.1</dt_mechanics>
		<dt_phenotype units="min">6</dt_phenotype>	
	</overall>
	
	<parallel>
		<omp_num_threads>10</omp_num_threads>
	</parallel> 
	
	<save>
		<folder>output</folder>  

		<full_data>
			<interval units="min">360</interval>
			<enable>true</enable>
		</full_data>
		
		<SVG>
			<interval units="min">20</interval>
			<enable>true</enable>
		</SVG>
		
		<legacy_data>
			<enable>false</enable>
		</legacy_data>
	</save>
	
	<options>
		<legacy_random_points_on_sphere_in_divide>false</legacy_random_points_on_sphere_in_divide>
        <virtual_wall_at_domain_edge>true</virtual_wall_at_domain_edge>
	</options>	

	<microenvironment_setup>
		<variable name="resource" units="dimensionless" ID="0">
        <physical_parameter_set>
          <diffusion_coefficient units="micron^2/min">100000.0</diffusion_coefficient>
          <decay_rate units="1/min">0.1</decay_rate>
        </physical_parameter_set>
        <initial_condition units="mmHg">1.0</initial_condition>
        <Dirichlet_boundary_condition units="mmHg" enabled="False">1.0</Dirichlet_boundary_condition>
        <Dirichlet_options><boundary_value ID="xmin" enabled="False">1.0</boundary_value>
          <boundary_value ID="xmax" enabled="False">1.0</boundary_value>
          <boundary_value ID="ymin" enabled="False">1.0</boundary_value>
          <boundary_value ID="ymax" enabled="False">1.0</boundary_value>
          <boundary_value ID="zmin" enabled="False">1.0</boundary_value>
          <boundary_value ID="zmax" enabled="False">1.0</boundary_value>
        </Dirichlet_options>
        </variable>

      <variable name="quorum" units="dimensionless" ID="1">
